<section id="contact" class="section contact">
    <div class="container">

        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mb-50">
                <h1 class="text-center mb-10">Contato</h1>
                <h2 class="text-center mb-50">Entre em contato conosco e faça já seu orçamento!</h2>
                <form action="/contact" method="POST">
                    <span>
                        <input type="text" name="name" minlength="2" maxlength="100" placeholder="NOME" required />
                        <input type="email" name="email" maxlength="200" placeholder="EMAIL" required />
                        <input type="text" name="subject" minlength="3" maxlength="200" placeholder="ASSUNTO"
                            required />
                    </span>
                    <span>
                        <textarea name="message" minlength="50" maxlength="1000" placeholder="MENSAGEM"
                            required></textarea>
                        <button type="submit">Enviar</button>
                    </span>

                </form>
            </div>
        </div>
    </div>
</section>
<section id="newsletter" class="section newsletter mb-50">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <h1 class="text-center mb-10">Newsletter</h1>
                <h2 class="text-center mb-50">Assine nossa newsletter para receber dicas, promoções e notícias da Mandela</h2>
                <form action="newsletter" method="POST">
                    <input type="text" name="name" placeholder="Digite seu nome" minlength="3" maxlength="200"
                        required />
                    <input type="email" name="email" placeholder="Digite seu email" minlength="10" maxlength="200"
                        required />
                    <button type="submit">Enviar</button>
                </form>
            </div>
        </div>
    </div>
</section>
